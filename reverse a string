import java.util.*;

public class HelloWorld {
    public static void main(String[] args) {
       String str="saam";
       char []ch=str.toCharArray();
       
       StringBuilder builder=new StringBuilder();
       
       for(int i=ch.length-1;i>=0;i--){
           builder.append(ch[i]);
           
           
       }
       System.out.println(builder);    
       
    }
}
